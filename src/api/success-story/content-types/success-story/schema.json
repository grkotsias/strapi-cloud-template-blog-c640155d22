{
  "kind": "collectionType",
  "collectionName": "success_stories",
  "info": {
    "singularName": "success-story",
    "pluralName": "success-stories",
    "displayName": "Success Stories"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "url": {
      "type": "uid",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "targetField": "title",
      "required": true
    },
    "successStoriesCategory": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::success-stories-category.success-stories-category",
      "inversedBy": "successStories"
    },
    "articlesTags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::articles-tag.articles-tag",
      "mappedBy": "successStories"
    },
    "description": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "thumbnail": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "content": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "aside": {
      "type": "dynamiczone",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "components": [
        "components.simple-link",
        "widgets.links-widget"
      ]
    },
    "seo": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "components.seo",
      "repeatable": false
    }
  }
}
